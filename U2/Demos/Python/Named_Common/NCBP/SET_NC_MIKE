SUBROUTINE SET_NC_MIKE(  GET_THIS, THE_ARRAY )
COMMON /MIKE/ FILE_VAR, DIMARRAY(10), DYNARRAY
BEGIN CASE
  CASE GET_THIS = "DIMARRAY"
    CONVERT @AM TO "~" IN THE_ARRAY
    THE_ARRAY = RAISE(THE_ARRAY)
    MATPARSE DIMARRAY FROM THE_ARRAY, "~"
    FOR A = 1 TO 10
        CRT A,DIMARRAY(A)
    NEXT A
  CASE GET_THIS = "DYNARRAY"
    DYNARRAY = THE_ARRAY
END CASE
RETURN
